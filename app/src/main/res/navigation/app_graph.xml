            app:destination="@id/dcUnloadingCongratulationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/dcForcedTerminationDetailsFragment"
        android:name="ru.wb.go.ui.dcunloadingforcedtermination.DcForcedTerminationDetailsFragment"
        android:label="@string/dc_unloading_forced_termination_details_label"
        tools:layout="@layout/dc_forced_termination_details_fragment" />

    <dialog
        android:id="@+id/dcLoadingHandleFragment"
        android:name="ru.wb.go.ui.dcloading.DcLoadingHandleFragment"
